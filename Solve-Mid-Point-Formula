def midpoint_formula(x1, y1, x2, y2):
    x_mid = (x1 + x2) / 2
    y_mid = (y1 + y2) / 2
    return (x_mid, y_mid)

x1 = float(input("Enter x1: "))
y1 = float(input("Enter y1: "))
x2 = float(input("Enter x2: "))
y2 = float(input("Enter y2: "))

result = midpoint_formula(x1, y1, x2, y2)
print("The midpoint is:", result)

